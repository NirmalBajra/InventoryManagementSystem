@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Stock Report";
}


<div class="container">
    <div class="page-inner">
        <div class="card">
            <div class="card-header">
                <h1>Stock Report</h1>
            </div>

            <div class="card-body">
                <table class="table">
                    <thead>
                        <tr>
                            <th>
                                Product Name
                            </th>
                            <th>
                                Category
                            </th>
                            <th>
                                Quantity
                            </th>
                            <th>
                                Unit Price
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.ProductName
                                </td>
                                <td>
                                    @item.CategoryName
                                </td>
                                <td>
                                    @item.Quantity
                                </td>
                                <td>
                                    @item.UnitPrice
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>